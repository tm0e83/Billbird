    "pinia": "^2.1.7",
    "sass": "^1.76.0",
    "vue": "^3.4.21",
    "vue-currency-input": "^3.1.0",
    "vue-router": "^4.3.0",
    "vue-tabler-icons": "^2.21.0",
    "vuedraggable": "^4.1.0",
    "vuefire": "^3.1.23"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
